{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "start-react",
            "type": "shell",
            "command": "npm",
            "args": ["start"],
            "options": {
                "cwd": "${workspaceFolder}/time_to_taste"
            },
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "new"
            },
            "isBackground": true,
            "problemMatcher": {
                "owner": "webpack",
                "pattern": {
                    "regexp": ".*"
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": ".*webpack.*",
                    "endsPattern": ".*compiled successfully.*"
                }
            }
        }
    ]
}
